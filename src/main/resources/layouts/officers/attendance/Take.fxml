<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Button?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            stylesheets="@../../../styles/custom.css"
            fx:controller="com.propscout.gui.controllers.officers.attendance.TakeController"
            prefHeight="400.0" prefWidth="600.0">

    <Button fx:id="refreshButton" text="Refresh" styleClass="primary-button" AnchorPane.topAnchor="8.0" AnchorPane.rightAnchor="8.0">
        <graphic>
            <FontIcon styleClass="secondary-icon" iconLiteral="fa-refresh"/>
        </graphic>
    </Button>

    <ListView fx:id="lecturesListView" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.topAnchor="48.0"
              AnchorPane.rightAnchor="0"/>

</AnchorPane>
